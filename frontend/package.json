{
  "name": "frontend",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build": "expo export --platform web --output-dir web-build --clear",
    "build:web": "expo export --platform web --output-dir web-build --clear",
    "deploy": "npm run build && netlify deploy --prod --dir web-build"
  },
  "dependencies": {
    "expo": "~53.0.20",
    "expo-document-picker": "^13.1.6",
    "expo-status-bar": "~2.2.3",
    "react": "19.0.0",
    "react-native": "0.79.5",
    "react-dom": "19.0.0",
    "react-native-web": "^0.20.0",
    "@expo/metro-runtime": "~5.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0"
  },
  "private": true
}
